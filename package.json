{
  "name": "fuzeui",
  "version": "0.1.0",
  "description": "Fuze UI - Frontend Framework with Bulma Css with Svelte.js ",
  "main": "dist/fuzeui.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/praneybehl/fuzeui.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dist": "rollup -c",
    "prepublish": "npm run dist",
    "postpublish": "git push && git push --tags",
    "clean": "rimraf dist",
    "build:style": "npm-run-all clean build:scss build:autoprefix",
    "build:scss": "node-sass src/global.scss -o dist",
    "build:autoprefix": "postcss --use autoprefixer -o dist/global.css dist/global.css"
  },
  "dependencies": {
    "svelte": "^1.12.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "captain-git-hook": "~1.0.5",
    "lodash.isobject": "^3.0.2",
    "lodash.tonumber": "^4.0.3",
    "node-sass": "^4.5.1",
    "npm-run-all": "^4.0.2",
    "postcss": "^5.2.16",
    "postcss-cli": "^3.0.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-sass": "^0.4.10",
    "rollup-plugin-svelte": "^1.6.1"
  },
  "keywords": [
    "fuzeui",
    "Frontend",
    "Framework",
    "UI",
    "framework",
    "blumacss",
    "svelte.js"
  ],
  "author": "Praney Behl <praneybehl@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/praneybehl/fuzeui/issues"
  },
  "homepage": "https://github.com/praneybehl/fuzeui#readme"
}
